---
extends:
  - "plugin:lodash-fp/recommended"
  - formidable/configurations/es6-test
  - formidable/configurations/es6-node

plugins:
  - lodash-fp

rules:
  func-style: off
  arrow-parens: off
  no-magic-numbers: off
  no-unused-expressions: off
  max-nested-callbacks: off
  no-invalid-this: off
  max-statements: off
